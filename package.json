{
  "name": "unplugin-raw",
  "version": "0.6.3",
  "packageManager": "pnpm@10.22.0",
  "description": "Transform file to a default-export string.",
  "type": "module",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-raw#readme",
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-raw/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-raw.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./api": "./dist/api.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./rspack": "./dist/rspack.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "esbuild": ">=0.25.0"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^2.3.10",
    "unplugin-utils": "^0.3.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.2.10",
    "@sxzz/prettier-config": "^2.2.5",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "rolldown": "^1.0.0-beta.50",
    "rollup": "^4.53.2",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
